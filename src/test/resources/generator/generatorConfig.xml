<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >  
<generatorConfiguration>  
    <!-- 引入配置文件 -->  
    <properties resource="generator/init.properties"/>
      
    <!-- 一个数据库一个context -->  
    <context id="infoGuardian">  
        <!-- 注释 -->  
        <commentGenerator >  
            <property name="suppressAllComments" value="true"/><!-- 是否取消注释 -->  
            <property name="suppressDate" value="true" /> <!-- 是否生成注释代时间戳-->  
        </commentGenerator>  
          
        <!-- jdbc连接 -->  
        <jdbcConnection driverClass="${jdbc_driver}"  
            connectionURL="${jdbc_url}" userId="${jdbc_user}"  
            password="${jdbc_password}" />  
          
        <!-- 类型转换 -->  
        <javaTypeResolver>  
            <!-- 是否使用bigDecimal， false可自动转化以下类型（Long, Integer, Short, etc.） -->  
            <property name="forceBigDecimals" value="false"/>  
        </javaTypeResolver>  
          
        <!-- 生成实体类地址 -->    
        <javaModelGenerator targetPackage="com.mlamp.rcsc.model" targetProject="${project}" > </javaModelGenerator>
          
        <!-- 生成mapxml文件 -->  
        <sqlMapGenerator targetPackage="com.mlamp.rcsc.dao" targetProject="${project}" >  </sqlMapGenerator>

        <!-- 生成mapxml对应client，也就是接口dao -->      
        <javaClientGenerator targetPackage="com.mlamp.rcsc.dao" targetProject="${project}" type="XMLMAPPER" ></javaClientGenerator>
          
        <!-- 配置表信息
        <table schema="${jdbc_user}" tableName="s_user"  
            domainObjectName="UserEntity" enableCountByExample="false"  
            enableDeleteByExample="false" enableSelectByExample="false"  
            enableUpdateByExample="false">  
            <ignoreColumn column="FRED" />  
            <columnOverride column="LONG_VARCHAR_FIELD" jdbcType="VARCHAR" />  
        </table>  
        <table tableName="business_define" enableCountByExample="false"  enableDeleteByExample="false"
        	enableSelectByExample="false" enableUpdateByExample="false">
        </table>
       -->
       <table tableName="rc_gxml_template" enableCountByExample="false"  enableDeleteByExample="false"
              enableSelectByExample="false" enableUpdateByExample="false">
       </table>
        <table tableName="rc_config" enableCountByExample="false"  enableDeleteByExample="false"
               enableSelectByExample="false" enableUpdateByExample="false">
        </table>
    </context>
</generatorConfiguration>  